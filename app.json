{
  "expo": {
    "name": "expensify-cards",
    "slug": "expensify-cards",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "expensifycards",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.expensifycards"
    },
    "android": {
      "image": "./assets/images/icon.png",
      "backgroundColor": "#051c09",
      "package": "com.anonymous.expensifycards"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#051c09"
        }
      ],
      [
        "expo-secure-store",
        {
          "configureAndroidBackup": true,
          "faceIDPermission": "We can add a custom message here as well!"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
